{
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist",
    "withGlobalTauri": false
  },
  "package": {
    "productName": "Leitstelle112",
    "version": "0.0.0"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "open": true
      },
      "fs": {
        "all": true,
        "scope": [
          "$DOCUMENT/*",
          "$DOCUMENT/Leitstelle112/*",
          "$DOCUMENT/Leitstelle112/userdata/*",
          "$DOCUMENT/Leitstelle112/default/*",
          "$DOCUMENT/Leitstelle112/default/calls/*",
          "$DOCUMENT/Leitstelle112/custom/*",
          "$DOCUMENT/Arcavigi Interactive/*",
          "$DOCUMENT/Arcavigi Interactive/Leitstelle112/*",
          "$DOCUMENT/Arcavigi Interactive/Leitstelle112/config/*"
        ]
      },
      "path": {
        "all": true
      }
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "Arcavigi Interactive",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ]
    },
    "security": {
      "csp": null
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "Leitstelle112-devbuild-v0.0.3",
        "width": 800,
        "height": 600
      }
    ]
  }
}